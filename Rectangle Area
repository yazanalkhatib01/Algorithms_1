float Calculat_Rectangle_Area(float num1, float num2) {
	return num1 * num2;
}

void Prent_Resulr(float REC_Area) {

	cout << "\nThe Reactangle Area is: " << REC_Area << endl;
}

int main()
{//15 Solution

